{
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0"
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "provider": {
      "name": "github",
      "options": {
        "slug": "marcmodin/dagger-go-example-app"
      }
    },
    "files-updater": {
      "names": [
        "npm"
      ]
    }
  }
}